<?xml version="1.0"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2014-05-23T10:53:32">
  <properties>
    <titles>
      <title>Bless the Lord, my soul</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Taize-i énekek" entry="63"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Bless the Lord, my soul,<br/>and bless God's holy name.<br/>Bless the Lord, my soul,<br/>who leads me into life.</lines>
    </verse>
    <verse name="v2">
      <lines>Áldott légy, Uram,<br/>szent neved áldja lelkem!<br/>Áldott légy, Uram,<br/>mert megváltottál már.</lines>
    </verse>
  </lyrics>
</song>
