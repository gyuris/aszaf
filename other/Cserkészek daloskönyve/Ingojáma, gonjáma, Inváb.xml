<?xml version="1.0" encoding="UTF-8"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2014-05-23T15:25:38">
  <properties>
    <titles>
      <title>Ingojáma, gonjáma, Inváb</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Cserkészek daloskönyve" entry="178"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ingojáma, gonjáma, Invábú!<br/>Jábú! Jábú! Invábú!</lines>
    </verse>
    <verse name="v2">
      <lines>Ő egy hős, egy oroszlán!<br/>Sőt ennél is több: víziló!</lines>
    </verse>
    <verse name="v3">
      <lines>Hős vad tigris, vad tigris, több annál:<br/>Király, király, víziló!</lines>
    </verse>
  </lyrics>
</song>
