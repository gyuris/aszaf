    <titles>
      <title>Nézem az eget</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Cserkészek daloskönyve" entry="48"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Nézem az eget, nézem az eget, ragyognak a csillagok,<br/>De ott köztük, de ott köztük legjobban csak egy ragyog.<br/>Az a csillag a magyar cserkészcsapatoknak a csillaga;<br/>Azért csillog a legjobban a többi közt egymaga.</lines>
    </verse>
  </lyrics>
</song>
