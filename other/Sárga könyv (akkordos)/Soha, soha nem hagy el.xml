  <properties>
    <titles>
      <title>Soha, soha nem hagy el</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <songbooks>
      <songbook name="Sárga akkordos" entry="150"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines><chord name="C"/>Soha, soha nem hagy <chord name="Am"/>el.<br/><chord name="Dm"/>Soha, soha nem hagy <chord name="G"/>el.<br/><chord name="C"/>Támaszkodj Rá <chord name="Am"/>testvérem,<br/>mert <chord name="Dm"/>Jézus soha <chord name="G"/>nem hagy <chord name="C"/>el!</lines>
    </verse>
    <verse name="v2">
      <lines><chord name="C"/>Szeretete sem fogy <chord name="Am"/>el.<br/><chord name="Dm"/>Szeretete sem fogy <chord name="G"/>el.<br/><chord name="C"/>Téged is tart <chord name="Am"/>erősen<br/>Jézus <chord name="Dm"/>Krisztus <chord name="G"/>szerete<chord name="C"/>te.</lines>
    </verse>
    <verse name="v3">
      <lines><chord name="C"/>Hála, hallelu<chord name="Am"/>ja!<br/><chord name="Dm"/>Hála, hallelu<chord name="G"/>ja!<br/><chord name="C"/>A mennyországhoz <chord name="Am"/>tartozunk,<br/>itt <chord name="Dm"/>csak vándor<chord name="G"/>lók va<chord name="C"/>gyunk.</lines>
    </verse>
  </lyrics>
</song>
