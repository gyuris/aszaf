  <properties>
    <titles>
      <title>Feltámadt, itt jár köztünk</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Sárga akkordos" entry="57"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines><chord name="E"/>Feltámadt, itt <chord name="H"/>jár köztünk,<br/><chord name="E"/>feltámadt, itt <chord name="H"/>él bennünk.<br/><chord name="F#"/>Feltámadt, hogy mi <chord name="A"/>élhessünk,<br/>hogy <chord name="G"/>élhes<chord name="H"/>sünk.</lines>
    </verse>
    <verse name="v2">
      <lines><chord name="E"/>Áldott légy <chord name="H"/>Istenünk!<br/><chord name="E"/>Te vagy az <chord name="H"/>életünk!<br/><chord name="F#"/>Áldott légy <chord name="A"/>Krisztusunk,<br/>meg<chord name="G"/>váltó U<chord name="H"/>runk!<br/>Glória, glória…</lines>
    </verse>
  </lyrics>
