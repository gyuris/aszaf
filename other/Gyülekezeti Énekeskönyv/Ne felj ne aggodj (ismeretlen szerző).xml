<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2019-10-01T15:03:37">
  <properties>
    <titles>
      <title>Ne felj ne aggodj</title>
    </titles>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
    <songbooks>
      <songbook name="Gyülekezeti Énekeskönyv" entry="325"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ne félj, ne aggódj, ne sírj, ne bánkódj: <br/>Ha tiéd Isten, tiéd már minden. <br/>Ne félj, ne aggódj, ne sírj, <br/>Ne bánkódj: elég Õ néked.</lines>
    </verse>
  </lyrics>
</song>
