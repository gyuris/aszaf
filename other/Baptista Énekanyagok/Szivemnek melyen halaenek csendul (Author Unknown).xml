<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2019-10-01T15:03:26">
  <properties>
    <titles>
      <title>Szivemnek melyen halaenek csendul</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Szívemnek mélyén hálaének csendül,<br/>Szívemnek mélyén téged áldalak.</lines>
    </verse>
    <verse name="v2">
      <lines>Új dallam zsong már szívem minden húrján.<br/>Új célom éltet, örök üdv vár rám.</lines>
    </verse>
  </lyrics>
</song>
