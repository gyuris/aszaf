  <lyrics>
    <verse name="v1">
      <lines>Bölcs vagy bolond asszony?</lines>
    </verse>
    <verse name="v2">
      <lines>Péld. 14: 1</lines>
    </verse>
    <verse name="v3">
      <lines>Bolond asszony:</lines>
    </verse>
    <verse name="v4">
      <lines>Ravasz asszony</lines>
    </verse>
    <verse name="v5">
      <lines>Ravasz asszony<br/>	Lenézõ asszony</lines>
    </verse>
    <verse name="v6">
      <lines>Ravasz asszony<br/>	Lenézõ asszony<br/>	Bálványimádó asszony</lines>
    </verse>
    <verse name="v7">
      <lines>Ravasz asszony<br/>	Lenézõ asszony<br/>	Bálványimádó asszony<br/>	Kapzsi asszony</lines>
    </verse>
    <verse name="v8">
      <lines>Ravasz asszony<br/>	Lenézo asszony<br/>	Bálványimádó asszony<br/>	Kapzsi asszony<br/>	Romlott asszony (Mark 6:17-27)</lines>
    </verse>
    <verse name="v9">
      <lines>Bölcs asszony:</lines>
    </verse>
    <verse name="v10">
      <lines>Imadkozó asszony</lines>
    </verse>
    <verse name="v11">
      <lines>Imadkozó asszony<br/>	Bûnbánó asszony</lines>
    </verse>
    <verse name="v12">
      <lines>Imadkozó asszony<br/>	Bûnbánó asszony<br/>	Hivõ asszony (2 Mozes 2:1-10)</lines>
    </verse>
  </lyrics>
