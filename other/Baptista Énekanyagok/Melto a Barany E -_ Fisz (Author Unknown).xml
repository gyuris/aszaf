  <lyrics>
    <verse name="v1">
      <lines>Méltó a Bárány,<br/>hogy vegyen dicsõséget,<br/>hódolatot, áldást<br/>és tisztességet!</lines>
    </verse>
    <verse name="c1">
      <lines>Szent az Õ neve<br/>Örökké, szent az Õ neve<br/>Örökké, szent az Õ neve<br/>Örökké!</lines>
    </verse>
  </lyrics>
