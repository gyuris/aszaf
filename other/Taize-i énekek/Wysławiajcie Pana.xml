  <properties>
    <titles>
      <title>Wysławiajcie Pana</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Taize-i énekek" entry="49"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Wysławiajcie Pana, O!<br/>Wysławiajcie Pana, O!<br/>Śpiewaj Panu cała ziemio,<br/>alleluja, alleluja!</lines>
    </verse>
    <verse name="v2">
      <lines>Neved énekeljük, ó!<br/>Neved énekeljük, ó!<br/>Áldunk téged mindig Urunk,<br/>allelúja, allelúja!</lines>
    </verse>
  </lyrics>
</song>
