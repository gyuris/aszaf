  <properties>
    <titles>
      <title>Kristus, din Ande i oss</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Taize-i énekek" entry="110"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Kristus, din Ande i oss<br/>är en källa med porlande vatten.</lines>
    </verse>
    <verse name="v2">
      <lines>Jézus, a szívünkben élsz,<br/>a te Lelked mibennünk az élet.</lines>
    </verse>
  </lyrics>
