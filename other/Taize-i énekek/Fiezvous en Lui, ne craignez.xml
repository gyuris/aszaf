<?xml version="1.0" encoding="UTF-8"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2015-06-09T16:47:39">
  <properties>
    <titles>
      <title>Fiezvous en Lui, ne craignez</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Taize-i énekek" entry="65"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Fiezvous en Lui, ne craignez pas.<br/>La paix de Dieu gardera vos cœurs.<br/>Fiezvous en Lui. Alleluia, alleluia!</lines>
    </verse>
    <verse name="v2">
      <lines>Mindig bizakodj, és sose félj.<br/>A föltámadt Krisztus benned él.<br/>Mindig bizakodj, allelúja, allelúja!</lines>
    </verse>
  </lyrics>
</song>
