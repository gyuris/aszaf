  <properties>
    <titles>
      <title>Bleibet hier und wachet mit</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Taize-i énekek" entry="45"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Bleibet hier und wachet mit mir.<br/>Wachet und betet, wachet und betet.</lines>
    </verse>
    <verse name="v2">
      <lines>Virrassz még és imádkozz még!<br/>Jézussal ébren, virrassz az éjben.</lines>
    </verse>
  </lyrics>
