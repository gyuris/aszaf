<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2014-05-23T15:25:37">
  <properties>
    <titles>
      <title>Elmegyek, elmegyek, el is van</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Cserkészek daloskönyve" entry="133"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Elmegyek, elmegyek, el is van vágyásom,<br/>Ebbe' rongyos kis tanyába' nincsen maradásom.</lines>
    </verse>
    <verse name="v2">
      <lines>Ha elmégy, ha elmégy, csak hozzám igaz légy,<br/>Igaz szereteted, babám, hamisra ne fordítsd!</lines>
    </verse>
  </lyrics>
</song>
