  <properties>
    <titles>
      <title>Szent vagy! Szent vagy! Szent</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <songbooks>
      <songbook name="Éneklő Egyház" entry="493"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Szent vagy! Szent vagy! Szent vagy! Mindenség Ura, Istene!<br/>Dicsőséged betölti a mennyet és a földet!<br/>Hozsanna a magasságban!<br/>Áldott, ki az Úr nevében jő!<br/>Hozsanna a magasságban!</lines>
    </verse>
  </lyrics>
