  <properties>
    <titles>
      <title>Ó szentséges lakoma</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <songbooks>
      <songbook name="Éneklő Egyház" entry="760"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ó szentséges lakoma!<br/>Krisztust vesszük rajta,<br/>felidézzük szenvedésének emlékét:<br/>bensőnket kegyelem tölti el,<br/>s a jövendő dicsőség záloga<br/>nekünk adatik, alleluja.</lines>
    </verse>
  </lyrics>
</song>
