  <properties>
    <titles>
      <title>Harmatozzatok egek, onnan</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <songbooks>
      <songbook name="Éneklő Egyház" entry="504"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Harmatozzatok egek, onnan felülről,<br/>és a felhők essék az Igazat.<br/>Nyíljék meg immár a föld,<br/>s teremje az Üdvöztőt.</lines>
    </verse>
  </lyrics>
