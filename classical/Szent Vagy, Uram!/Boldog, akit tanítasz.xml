    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Szent Vagy, Uram!" entry="233c"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Boldog, akit tanítasz,<br/>Ó megváltó Jézusom!<br/>Bízva megy Tefeléd Utadon.</lines>
    </verse>
    <verse name="v2">
      <lines>Üdvösségre nincsen út,<br/>Nincs máshol, csak tevéled,<br/>Te vagy út, igazság és élet.</lines>
    </verse>
  </lyrics>
</song>
