<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2014-05-23T16:07:48">
  <properties>
    <titles>
      <title>köznapi Introitus</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Graduale Hungaricum" entry="102"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Körülvettek engem a halál kínjai,<br/>az alvilág fájdalmai körülvettek engem.</lines>
    </verse>
    <verse name="v2">
      <lines>Szorongatásomban hívtam az Urat,<br/>s meghallotta szavamat szent templomából.</lines>
    </verse>
  </lyrics>
</song>
