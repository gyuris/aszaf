<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2015-06-09T16:45:47">
  <properties>
    <titles>
      <title>Húsvét V. Introitus</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Graduale Hungaricum" entry="201"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Énekeljetek az Úrnak új éneket;<br/>csodadolgokat cselekedett, alleluja.</lines>
    </verse>
    <verse name="v2">
      <lines>Kinyilatkoztatta igazságát, alleluja,<br/>a nemzetek színe előtt, alleluja.</lines>
    </verse>
  </lyrics>
</song>
