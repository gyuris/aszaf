  <properties>
    <titles>
      <title>XV. Communio [GRAD 100]</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Graduale Hungaricum" entry="XV"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>A veréb házat talál magának,<br/>s a gerlice is fészket, hová fiait helyezze:</lines>
    </verse>
    <verse name="v2">
      <lines>Én a te oltárodnál, seregek Ura,<br/>én királyom és Istenem!</lines>
    </verse>
  </lyrics>
</song>
