    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Graduale Hungaricum" entry="364"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Adj békét azoknak, kik reád várnak,<br/>hogy igazmondóknak bizonyuljanak prófétáid!</lines>
    </verse>
    <verse name="v2">
      <lines>Hallgasd meg, Uram, szolgádat,<br/>s népednek, Izráelnek könyörgését!</lines>
    </verse>
  </lyrics>
