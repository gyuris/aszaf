<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2015-06-09T16:45:48">
  <properties>
    <titles>
      <title>Mindenszentek Introitus [ÉE 646]</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Graduale Hungaricum" entry="425"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Örvendezzünk, mindnyájan az Úrban,<br/>ünnepi napot ülvén minden szentek tiszteletére;</lines>
    </verse>
    <verse name="v2">
      <lines>kiknek ünnepén örülnek az angyalok,<br/>és magasztalják az Istennek Fiát.</lines>
    </verse>
  </lyrics>
</song>
