<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2014-05-23T15:45:11">
  <properties>
    <titles>
      <title>Szent vagy</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <songbooks>
      <songbook name="Imák és válaszok" entry="20"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Szent vagy, szent vagy, szent vagy mindenség Ura, Istene!<br/>Dicsőséged betölti a mennyet és a földet.<br/>Hozsanna a magasságban!<br/>Áldott, aki jön az Úr nevében.<br/>Hozsanna a magasságban!</lines>
    </verse>
  </lyrics>
</song>
