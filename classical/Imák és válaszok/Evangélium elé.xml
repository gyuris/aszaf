<?xml version="1.0" encoding="UTF-8"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2014-05-23T15:45:10">
  <properties>
    <titles>
      <title>Evangélium elé</title>
    </titles>
    <verseOrder>v1 v2 v3 v4 v5</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Imák és válaszok" entry="13"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Dicsőség és hála legyen * neked, Urunk Jézus!</lines>
    </verse>
    <verse name="v2">
      <lines>Áldunk téged, Krisztus * örök dicsőség néked!</lines>
    </verse>
    <verse name="v3">
      <lines>Örök dicsőség Ura, Királya: * Krisztus, téged dicsérünk.</lines>
    </verse>
    <verse name="v4">
      <lines>Krisztus, Isten igéje, * néked zeng a dicsőítés.</lines>
    </verse>
    <verse name="v5">
      <lines>Dicsérünk, Jézus, * zengő énekszóval.</lines>
    </verse>
  </lyrics>
</song>
