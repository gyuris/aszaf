<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2015-06-09T16:45:09">
  <properties>
    <titles>
      <title>Táplálta őket a gabona</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Dicsérjétek az Urat!" entry="131b"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Táplálta őket a gabona javával, s a kősziklából jóllakatta őket mézzel.<br/>Alleluja, alleluja.</lines>
    </verse>
    <verse name="v2">
      <lines>Mindenek szemei Tebenned bíznak, Uram,<br/>(*)<br/>mert Te adsz nekik eledelt alkalmas időben.</lines>
    </verse>
  </lyrics>
</song>
