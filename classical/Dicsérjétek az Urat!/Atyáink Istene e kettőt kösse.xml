  <properties>
    <titles>
      <title>Atyáink Istene e kettőt kösse</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Dicsérjétek az Urat!" entry="321b"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Atyáink Istene e kettőt kösse össze,<br/>Mert megkönyörült rajtuk, kik egyedül voltak.</lines>
    </verse>
    <verse name="v2">
      <lines>Boldogok, akik az Urat félik,<br/>(*)<br/>ki az Ő útjain járnak.</lines>
    </verse>
  </lyrics>
</song>
