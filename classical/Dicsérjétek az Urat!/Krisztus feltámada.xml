  <properties>
    <titles>
      <title>Krisztus feltámada</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Dicsérjétek az Urat!" entry="98"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Krisztus feltámada<br/>Mind ő nagy kínjából,<br/>Ezen mi is örüljünk,<br/>Krisztus legyen reményünk,<br/>Kirje’lejszon.</lines>
    </verse>
    <verse name="v2">
      <lines>Ó kegyes Mária,<br/>Mennyei szép rózsa,<br/>Kérjed, hogy szent Szülötted<br/>Üdvözítse lelkünket,<br/>Kirje’lejszon.</lines>
    </verse>
  </lyrics>
</song>
