  <properties>
    <titles>
      <title>O salutaris Hostia, quae</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Dicsérjétek az Urat!" entry="156b"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>O salutaris Hostia, quae caeli pandis ostium:<br/>bella premunt hostilia, da robur, fer auxilium!</lines>
    </verse>
    <verse name="v2">
      <lines>Uni trinoque Domino sit sempiterna gloria,<br/>qui vitam sine termino nobis donet in patria.</lines>
    </verse>
  </lyrics>
</song>
