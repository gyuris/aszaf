<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2015-06-09T16:45:13">
  <properties>
    <titles>
      <title>Hajtsd hozzám Uram, füledet</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Dicsérjétek az Urat!" entry="391"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Hajtsd hozzám Uram, füledet, s hallgass meg engem,<br/>szabadítsd meg szolgádat, mert tebenned bízik.</lines>
    </verse>
    <verse name="v2">
      <lines>Jó dolog az Urat dicsérni,<br/>(*)<br/>zsoltározni a te nevednek, ó Fölséges!</lines>
    </verse>
  </lyrics>
</song>
