<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2015-06-09T16:45:10">
  <properties>
    <titles>
      <title>Jézusom isteni teste</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <songbooks>
      <songbook name="Dicsérjétek az Urat!" entry="186"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jézusom isteni teste,<br/>színed elé jövök esdve,<br/>lelkem az égbe vezesd be!</lines>
    </verse>
    <verse name="v2">
      <lines>Jézusom, isteni Bárány,<br/>égi kegyelmeid árán<br/>menny örök élete vár rám.</lines>
    </verse>
  </lyrics>
</song>
