<?xml version="1.0"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2014-05-23T16:06:18">
  <properties>
    <titles>
      <title>Isten házába indulunk</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Zsoltár - vasárnapok" entry="1"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Isten házába indulunk,<br/>* örömtől dobban a szívünk.</lines>
    </verse>
    <verse name="v2">
      <lines>Téríts meg minket, Istenünk,<br/>* ragyogtasd ránk arcodat, és szabadok leszünk!</lines>
    </verse>
    <verse name="v3">
      <lines>Hozzád emelem a lelkem,<br/>* én Uram, Istenem.</lines>
    </verse>
  </lyrics>
</song>
