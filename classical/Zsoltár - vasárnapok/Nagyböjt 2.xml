  <properties>
    <titles>
      <title>Nagyböjt 2.</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Zsoltár - vasárnapok" entry="40"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Kegyes szemed legyen rajtunk,<br/>* tebenned van bizodalmunk.</lines>
    </verse>
    <verse name="v2">
      <lines>Az Úr színe előtt járok,<br/>* az élők földjén.</lines>
    </verse>
    <verse name="v3">
      <lines>Az Úr az én világosságom,<br/>* és ő az én üdvösségem.</lines>
    </verse>
  </lyrics>
