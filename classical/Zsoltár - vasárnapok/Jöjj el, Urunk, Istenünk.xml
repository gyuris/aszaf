<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2014-05-23T16:06:18">
  <properties>
    <titles>
      <title>Jöjj el, Urunk, Istenünk</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <songbooks>
      <songbook name="Zsoltár - vasárnapok" entry="3"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jöjj el, Urunk, Istenünk:<br/>* üdvözíts minket!</lines>
    </verse>
    <verse name="v2">
      <lines>Lelkem ujjongva hirdeti<br/>* Istenemnek dicsőségét.</lines>
    </verse>
    <verse name="v3">
      <lines>Ujjongj és zengjél háladalt,<br/>* mert nagy és szent a te Megváltód.</lines>
    </verse>
  </lyrics>
</song>
