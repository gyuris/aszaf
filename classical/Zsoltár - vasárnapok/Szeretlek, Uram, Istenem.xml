<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2014-05-23T16:06:18">
  <properties>
    <titles>
      <title>Szeretlek, Uram, Istenem</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <songbooks>
      <songbook name="Zsoltár - vasárnapok" entry="33"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Szeretlek, Uram, Istenem,<br/>* én erősségem.</lines>
    </verse>
    <verse name="v2">
      <lines>Hatalmas dolgot művelt az Úr vélünk,<br/>* azért szívből ujjongunk.</lines>
    </verse>
    <verse name="v3">
      <lines>A szegény felkiáltott az Istenhez,<br/>* és Isten meghallgatta őt.</lines>
    </verse>
  </lyrics>
</song>
