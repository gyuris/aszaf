<?xml version="1.0" encoding="UTF-8"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="ConvDiatarOpenlyrics" modifiedIn="ConvDiatarOpenlyrics" modifiedDate="2014-05-23T16:06:18">
  <properties>
    <titles>
      <title>Urunk, édes Istenünk</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <songbooks>
      <songbook name="Zsoltár - vasárnapok" entry="19"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Urunk, édes Istenünk,<br/>* mennyire jó vagy hozzánk!</lines>
    </verse>
    <verse name="v2">
      <lines>Az Úr nékem pásztorom,<br/>* ínséget nem kell látnom.</lines>
    </verse>
    <verse name="v3">
      <lines>Uram, ki lakhat<br/>* a te hajlékodban?</lines>
    </verse>
  </lyrics>
</song>
