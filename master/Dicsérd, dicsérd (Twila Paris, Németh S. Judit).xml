    <titles>
      <title lang="hu">Dicsérd, dicsérd</title>
      <title lang="en">Praise Him</title>
    </titles>
    <authors>
      <author type="words">Twila Paris</author>
      <author type="music">Twila Paris</author>
      <author lang="hu" type="translation">Németh S. Judit</author>
    </authors>
    <songbooks>
      <songbook entry="64" name="Szent András énekfüzet" />
    </songbooks>
