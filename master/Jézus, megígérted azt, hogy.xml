    <titles>
      <title>Jézus, megígérted azt, hogy</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <songbooks>
      <songbook name="Zöld könyv" entry="309"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jézus, megígérted azt, hogy<br/>elküldöd nekünk Szent Lelkedet.<br/>Oh, jöjj, hadd vigyük a fényedet,<br/>és mindig áldjuk szent neved.</lines>
    </verse>
  </lyrics>
