    <titles>
      <title>Jöjj Szentlélek, szállj le rám</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <songbooks>
      <songbook name="Zöld könyv" entry="220"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jöjj Szentlélek, szállj le rám,<br/>Örök boldog fénysugár.<br/>Vezesd életem a szűk ösvényen,<br/>Segíts levetni régi ruhám!</lines>
    </verse>
  </lyrics>
