  <properties>
    <titles>
      <title lang="hu">Itt vagyok, várok</title>
      <title lang="en">Eagle’s Wings</title>
    </titles>
    <authors>
      <author type="words">Reuben Morgan</author>
      <author type="music">Reuben Morgan</author>
      <author lang="hu" type="translation">Bagyinszky Judit</author>
    </authors>
    <songbooks>
      <songbook entry="277" name="Szent András énekfüzet" />
    </songbooks>
    <copyright>© 1998 Hillsong Music Publishing</copyright>
    <verseOrder>v1 v2 c1</verseOrder>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Itt vagyok, várok,<br />maradj, Uram, velem,<br />itt vagyok, vágyom Reád!</lines>
    </verse>
    <verse name="v2">
      <lines>Szerelmed elrejt,<br />térdre kényszerít,<br />amikor köztünk megjelensz.</lines>
    </verse>
    <verse name="c1">
      <lines>Te élj bennem, uralkodj szívemben,<br />hadd érezzem erődet, amely szárnyra emel!</lines>
    </verse>
  </lyrics>
</song>
