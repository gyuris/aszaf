  <properties>
    <titles>
      <title>Mert Te, Uram</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
    <songbooks>
      <songbook name="Sárga könyv" entry="132"/>
	  <songbook name="Kék Könyv" entry="145"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Mert Te, Uram,<br/>fenséges vagy a földön, és<br/>felmagasztaltattál<br/>minden isten fölött!</lines>
    </verse>
    <verse name="c1">
      <lines>Imádunk Téged, imádunk Téged,<br/>Imádunk Téged, ó Urunk!</lines>
    </verse>
  </lyrics>
</song>
