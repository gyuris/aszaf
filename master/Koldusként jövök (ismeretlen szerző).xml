  <properties>
    <titles>
      <title>Koldusként jövök</title>
    </titles>
    <verseOrder>c1 v1 c1 v2 c1</verseOrder>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
    <songbooks>
      <songbook name="Sárga könyv" entry="106"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Koldusok királya, Jézus,<br/>mentsd meg a lelkem!</lines>
    </verse>
    <verse name="c1">
      <lines>Koldusként jövök Hozzád, koldus vagyok.</lines>
    </verse>
    <verse name="v2">
      <lines>Rongyaimat Néked adom,<br/>fogadd el hát,<br/>hadd legyen életem Tiéd!</lines>
    </verse>
  </lyrics>
