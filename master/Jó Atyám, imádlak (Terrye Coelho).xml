  <properties>
    <titles>
      <title>Jó Atyám, imádlak</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author>Terrye Coelho</author>
    </authors>
    <songbooks>
      <songbook name="Sárga könyv" entry="95"/>
      <songbook name="Kék könyv" entry="106"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jó Atyám imádlak, leteszem életem.<br/>Úgy szeretlek!</lines>
    </verse>
    <verse name="v2">
      <lines>Úr Jézus imádlak, leteszem életem.<br/>Úgy szeretlek!</lines>
    </verse>
    <verse name="v3">
      <lines>Szentlélek, úgy várlak, leteszem életem.<br/>Úgy szeretlek!</lines>
    </verse>
  </lyrics>
</song>
