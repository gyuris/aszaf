<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 1.9.11" modifiedIn="OpenLP 1.9.11" modifiedDate="2012-12-03T21:35:15">
  <properties>
    <titles>
      <title>Ó, jöjjetek, örvendjünk</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
    <songbooks>
      <songbook name="Sárga könyv" entry="134"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ó, jöjjetek, örvendjünk,<br/>lelkesen tapsoljunk,<br/>énekeljünk az Úrnak!</lines>
    </verse>
    <verse name="c1">
      <lines>Isten küldötte el nékünk<br/>szabadítóját.<br/>A népek előtt megjelentette<br/>az Ő igazságát.</lines>
    </verse>
  </lyrics>
</song>
