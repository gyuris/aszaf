<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 1.9.11" modifiedIn="OpenLP 1.9.11" modifiedDate="2012-12-03T19:04:34">
  <properties>
    <titles>
      <title>A föld, amelyre</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
    <songbooks>
      <songbook name="Sárga könyv" entry="44"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>A föld, amelyre lábad lép,<br/>réges-régen a tied,<br/>mert én adtam azt neked.</lines>
    </verse>
    <verse name="c1">
      <lines>Nem illetheti az ellenség,<br/>nem lesz sohasem idegené.<br/>Nem lakhat ott csak az a nép,<br/>akié a szövetség.</lines>
    </verse>
  </lyrics>
</song>
