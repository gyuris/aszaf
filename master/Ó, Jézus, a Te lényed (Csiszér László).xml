      <title lang="hu">Ó, Jézus, a Te lényed</title>
    </titles>
    <authors>
      <author type="words">Csiszér László</author>
      <author type="music">Csiszér László</author>
    </authors>
    <songbooks>
      <songbook entry="208" name="Szent András énekfüzet" />
      <songbook name="Zöld könyv" entry="416"/>
