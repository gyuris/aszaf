  <properties>
    <titles>
      <title>Fölém hajló jóságodra</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
    <songbooks>
      <songbook name="Sárga könyv" entry="66"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Fölém hajló jóságodra emlékezz, Uram!<br/>Irgalmadra, mi örökké igazság marad.</lines>
    </verse>
    <verse name="c1">
      <lines>Mosogass meg a bűneimtől, tisztogass meg a vétkeimtől!<br/>Vétkemet elismerem, bűnöm előttem áll szüntelen.</lines>
    </verse>
  </lyrics>
</song>
